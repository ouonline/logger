AR := ar
CC := gcc

ifeq ($(debug), y)
	CFLAGS := -g
else
	CFLAGS := -O2 -DNDEBUG
endif
CFLAGS := $(CFLAGS) -Wall -Werror -Wextra -fPIC

TARGET := test_logger

.PHONY: all clean

all: $(TARGET)

.PHONY: __omake_dep__1

__omake_dep__1:
	$(MAKE) debug=$(debug) liblogger.a -C ..

test_logger_OBJS := test_logger.c.test_logger.o

test_logger_INCLUDE := -I../..

test_logger_LIBS := ../liblogger.a ../../utils/libutils.a -lpthread

test_logger.c.test_logger.o: test_logger.c
	$(CC) $(CFLAGS) $(test_logger_INCLUDE) -c $< -o $@

test_logger: $(test_logger_OBJS) | __omake_dep__1
	$(CC) $(CFLAGS) -o $@ $^ $(test_logger_LIBS)

clean:
	rm -f $(TARGET) $(test_logger_OBJS)
